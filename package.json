{
  "name": "mcp-8thwall",
  "version": "0.2.10",
  "private": true,
  "type": "module",
  "description": "MCP server for 8th Wall (docs or API modes)",
  "main": "dist/index.js",
  "license": "MIT",
  "author": "Dwayne Paisley-Marshall",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/superdwayne/8thwallmcp.git"
  },
  "bugs": {
    "url": "https://github.com/superdwayne/8thwallmcp/issues"
  },
  "homepage": "https://github.com/superdwayne/8thwallmcp#readme",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "node --loader ts-node/esm src/index.ts",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "http": "node dist/http.js",
    "dev:http": "node --loader ts-node/esm src/http.ts",
    "pack:dxt": "npm run build && npx @anthropic-ai/dxt pack",
    "install:dxt": "npm install -g @anthropic-ai/dxt"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.2.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^22.18.8",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
